{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "comma-style": "off",
    "no-console": "off",
    "linebreak-style": "off",
    "class-methods-use-this": "off"
  }
}
